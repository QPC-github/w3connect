<% if (user && typeof interestMarker != "undefined" && interestMarker) { %>
<% var isInterested = new RegExp("^"  + event.interested.join("|") + "$"); %>
<a class="interest" href="<%= baseurl %>/schedule/events/<%= event.slug %>" title="Toggle interest on talk"><img style='vertical-align: middle' src="<%= baseurl %>/img/star<%= ( isInterested.test(user._id) ?  "" : "-off" ) %>.png" alt="Not marked as interested"></a>
<% } %>
<% if (typeof showTime != "undefined" && showTime) { %>
<% var days = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"]; %>
<%= days[event.timeStart.getDay()] %> <%- partial("schedule/timeslot.ejs", {timeStart: event.timeStart, timeEnd: event.timeEnd}) %>
<% } %>
<% if (event.room && event.room.shortname) { %>
<a href="<%= baseurl %>/locations/<%= event.room.shortname %>"><%= event.room.name %></a>: 
<% } %> 
<strong><%= event.name %></strong>
<% if (event.presenters) { %>
, by <%= event.presenters %>
<% } %>
<% if (event.confidentiality && event.confidentiality == 'member') { %>
 <strong>W3C Member-only</strong>
<% } %>
<% if (event.observers && event.observers == 'no') { %>
, <strong>no observers allowed</strong>
<% } %>
<em class="interested" data-eventid="<%= event.slug %>"><% if (event.interested.length) { %>
 (<span class="counter"><%= event.interested.length %></span> interested to attend)
<% } %></em>